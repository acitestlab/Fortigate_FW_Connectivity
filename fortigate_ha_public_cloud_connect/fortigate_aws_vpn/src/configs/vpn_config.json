{
  "vpn_type": "ipsec",
  "authentication": {
    "method": "pre-shared-key",
    "pre_shared_key": "your_pre_shared_key_here"
  },
  "ike_version": 2,
  "local_gateway": {
    "ip": "your_local_gateway_ip_here",
    "subnet": "your_local_subnet_here"
  },
  "remote_gateway": {
    "ip": "your_aws_vpn_gateway_ip_here",
    "subnet": "your_aws_subnet_here"
  },
  "tunnel_settings": {
    "encryption": "aes256",
    "integrity": "sha256",
    "lifetime": 3600
  },
  "keepalive": {
    "enabled": true,
    "interval": 30,
    "timeout": 120
  }
}